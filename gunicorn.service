[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=main
Group=www-data
WorkingDirectory=/home/main/app
ExecStart=/home/main/app/myprojectenv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/sammy/myproject/myproject.sock myproject.wsgi:application

[Install]
WantedBy=multi-user.target
